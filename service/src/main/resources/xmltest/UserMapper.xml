<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ly.traffic.middleplatform.domain.createorder.repository.mapper.UserMapper">
    <resultMap id="BaseResultMap" type="com.ly.traffic.middleplatform.domain.createorder.repository.po.User">
        <constructor>
            <idArg column="id" javaType="_long" jdbcType="BIGINT" />
            <arg column="name" javaType="java.lang.String" jdbcType="VARCHAR" />
            <arg column="mobile" javaType="java.lang.String" jdbcType="VARCHAR" />
            <arg column="password" javaType="java.lang.String" jdbcType="CHAR" />
            <arg column="role" javaType="java.lang.String" jdbcType="VARCHAR" />
        </constructor>
    </resultMap>

    <sql id="Base_Column_List">
    id, name, mobile, password, role
  </sql>
    <insert id="add">
        insert into test_demo
        (id, name, mobile, password, role)
        values
        (#{id}, #{name},#{mobile},#{password},#{role})
    </insert>

    <select id="selectByPrimaryKey" parameterType="_long" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from test_demo
        where id = #{id,jdbcType=BIGINT}
    </select>
</mapper>
